{
    "name": "better-toml",
    "displayName": "better toml",
    "description": "TOML Language support",
    "version": "0.0.1",
    "publisher": "bungcip",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.6",
        "vscode": "^1.0.3",
        "mocha": "^3.1.2",
        "@types/node": "^6.0.46",
        "@types/mocha": "^2.2.32"
    }
}